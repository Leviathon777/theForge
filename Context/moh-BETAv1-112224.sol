pragma solidity ^0.8.28;


/* Medals of Honor Smart Contract */
contract MEDALS_OF_HONOR_by_XdRiP is
    ERC721Enumerable,
    Ownable,
    ReentrancyGuard
{
    using Counters for Counters.Counter;

    /* Token supply limits */
    uint256 public constant commonSupply = 10000;
    uint256 public constant uncommonSupply = 5000;
    uint256 public constant rareSupply = 2500;
    uint256 public constant epicSupply = 1000;
    uint256 public constant legendarySupply = 500;
    uint256 public constant eternalSupply = 20;

    /* forge prices */
    uint256 public commonPrice = 0.5 ether;
    uint256 public uncommonPrice = 1 ether;
    uint256 public rarePrice = 1.5 ether;
    uint256 public epicPrice = 2 ether;
    uint256 public legendaryPrice = 2.5 ether;
    uint256 public eternalPrice = 200 ether;

    /* Tascas for revenue distribution */
    address payable public operatingTasca;

    /* Multi-padrone setup */
    address[] public padroneTascas;
    mapping(address => bool) public isPadrone;

    uint16 public padronesPercentage = 70; 
    uint16 public operatingCostsPercentage = 30; 
    

    Counters.Counter private _commonCounter;
    Counters.Counter private _uncommonCounter;
    Counters.Counter private _rareCounter;
    Counters.Counter private _epicCounter;
    Counters.Counter private _legendaryCounter;
    Counters.Counter private _eternalCounter;

    string private _contractURI;

    mapping(address => mapping(uint256 => bool)) private _ownsCommonPerCycle;
    mapping(address => mapping(uint256 => bool)) private _ownsUncommonPerCycle;
    mapping(address => mapping(uint256 => bool)) private _ownsRarePerCycle;
    mapping(address => mapping(uint256 => bool)) private _ownsEpicPerCycle;
    mapping(address => mapping(uint256 => bool)) private _ownsLegendaryPerCycle;
    mapping(address => bool) private _ownsEternal;
    mapping(address => uint256) private _userCycles;

    /* @dev mapping to store tokenURIs */
    mapping(uint256 => string) private _tokenURIs;

    /* @dev mapping token ID to IPFS hash */
    mapping(uint256 => string) private _tokenIPFSHashes;

    /* @dev mapping from address to last forge timestamp */
    mapping(address => mapping(uint256 => uint256)) private _lastforgeTimestamp;

    event TransferFailed(
        address indexed recipient, 
        uint256 amount
        );

    event Commonforged(
        address indexed padrone,
        uint256 tokenId,
        string ipfsHash
    );
    event Uncommonforged(
        address indexed padrone,
        uint256 tokenId,
        string ipfsHash
    );
    event Rareforged(
        address indexed padrone, 
        uint256 tokenId, 
        string ipfsHash
    );
    event Epicforged(
        address indexed padrone, 
        uint256 tokenId, 
        string ipfsHash
    );
    event Legendaryforged(
        address indexed padrone,
        uint256 tokenId,
        string ipfsHash
    );
    event Eternalforged(
        address indexed padrone,
        uint256 tokenId,
        string ipfsHash
    );
    event CycleCompleted(
        address indexed padrone, 
        uint256 cycleNumber
    );

   constructor(
    address payable _padroneTasca1,
    address payable _padroneTasca2,
    address payable _padroneTasca3,
    address payable _padroneTasca4,
    address payable _operatingTasca,
    address initialOwner 
) ERC721("Medals of Honor", "MOH") Ownable(initialOwner) {
    isPadrone[_padroneTasca1] = true;
    isPadrone[_padroneTasca2] = true;
    isPadrone[_padroneTasca3] = true;
    isPadrone[_padroneTasca4] = true;
    padroneTascas = [
        _padroneTasca1,
        _padroneTasca2,
        _padroneTasca3,
        _padroneTasca4
    ];
    operatingTasca = _operatingTasca;
}

    modifier onlyPadrones() {
        require(isPadrone[msg.sender], "Caller is not a padrone");
        _;
    }

    function setAllocationValue(uint16 _padronesPercentage, uint16 _operatingCostsPercentage) public onlyPadrones {
        require(_padronesPercentage + _operatingCostsPercentage == 100, "Total must be 100");
        padronesPercentage = _padronesPercentage;
        operatingCostsPercentage = _operatingCostsPercentage;
    }


    function _allocationProcessing() internal nonReentrant {
        uint256 totalFunds = address(this).balance;
        require(totalFunds > 0, "No funds to distribute");
        uint256 padronesShare = (totalFunds * padronesPercentage) / 100;
        uint256 operatingShare = (totalFunds * operatingCostsPercentage) / 100;

        uint256 eachPadroneShare = padronesShare / padroneTascas.length;
        for (uint256 i = 0; i < padroneTascas.length; i++) {
            _safeTransfer(payable(padroneTascas[i]), eachPadroneShare);
            
        }

        _safeTransfer(operatingTasca, operatingShare);
        uint256 allocated = padronesShare + operatingShare;
        uint256 remainingBalance = totalFunds - allocated;

        if (remainingBalance > 0) {
            _safeTransfer(operatingTasca, remainingBalance);
            
        }
    }

    /* safe transfer function */
    function _safeTransfer(address payable recipient, uint256 amount) internal {
        (bool success, ) = recipient.call{value: amount}("");
        if (!success) {
            emit TransferFailed(recipient, amount);
        }
    }

    function _exists(uint256 tokenId) internal view virtual returns (bool) {
        return _ownerOf(tokenId) != address(0);
    }

    function forgeCommon(string memory ipfsHash) public payable {
        forgeDOT(ipfsHash, 1);
    }

    function forgeUncommon(string memory ipfsHash) public payable {
        forgeDOT(ipfsHash, 2);
    }

    function forgeRare(string memory ipfsHash) public payable {
        forgeDOT(ipfsHash, 3);
    }

    function forgeEpic(string memory ipfsHash) public payable {
        forgeDOT(ipfsHash, 4);
    }

    function forgeLegendary(string memory ipfsHash) public payable {
        forgeDOT(ipfsHash, 5);
    }

    function forgeEternal(string memory ipfsHash) public payable {
        forgeDOT(ipfsHash, 6);
    }

    function forgeDOT(string memory ipfsHash, uint256 tier) internal {
        uint256 price;
        uint256 supply;
        Counters.Counter storage counter;
        uint256 tokenId;
        uint256 startingId;
        uint256 currentCycle = _userCycles[msg.sender];

        if (tier == 1) {
            price = commonPrice;
            supply = commonSupply;
            counter = _commonCounter;
            startingId = 1;
            require(
                !_ownsCommonPerCycle[msg.sender][currentCycle],
                "COMMON already forged"
            );
        } else if (tier == 2) {
            price = uncommonPrice;
            supply = uncommonSupply;
            counter = _uncommonCounter;
            startingId = 10001;
            require(
                _ownsCommonPerCycle[msg.sender][currentCycle],
                "Must own COMMON to forge UNCOMMON"
            );
            require(
                !_ownsUncommonPerCycle[msg.sender][currentCycle],
                "UNCOMMON already forged"
            );
        } else if (tier == 3) {
            price = rarePrice;
            supply = rareSupply;
            counter = _rareCounter;
            startingId = 15001;
            require(
                _ownsUncommonPerCycle[msg.sender][currentCycle],
                "Must own UNCOMMON to forge RARE"
            );
            require(
                !_ownsRarePerCycle[msg.sender][currentCycle],
                "RARE already forged"
            );
        } else if (tier == 4) {
            price = epicPrice;
            supply = epicSupply;
            counter = _epicCounter;
            startingId = 17501;
            require(
                _ownsRarePerCycle[msg.sender][currentCycle],
                "Must own RARE to forge EPIC"
            );
            require(
                !_ownsEpicPerCycle[msg.sender][currentCycle],
                "EPIC already forged"
            );
        } else if (tier == 5) {
            price = legendaryPrice;
            supply = legendarySupply;
            counter = _legendaryCounter;
            startingId = 18501;
            require(
                _ownsEpicPerCycle[msg.sender][currentCycle],
                "Must own EPIC to forge LEGENDARY"
            );
            require(
                !_ownsLegendaryPerCycle[msg.sender][currentCycle],
                "LEGENDARY already forged"
            );
        } else if (tier == 6) {
            price = eternalPrice;
            supply = eternalSupply;
            counter = _eternalCounter;
            startingId = 20001;
            require(!_ownsEternal[msg.sender], "ETERNAL already forged");
        } else {
            revert("INVALID_TIER");
        }

        require(msg.value >= price, "INSUFFICIENT_BNB");
        require(counter.current() < supply, "SUPPLY_EMPTY");

        tokenId = counter.current() + startingId;
        _forge(msg.sender, tokenId);
        _tokenIPFSHashes[tokenId] = ipfsHash;
        counter.increment();
        _lastforgeTimestamp[msg.sender][tier] = block.timestamp;
        _setTokenURI(tokenId, ipfsHash);

        if (tier == 1) {
            _ownsCommonPerCycle[msg.sender][currentCycle] = true;
            emit Commonforged(msg.sender, tokenId, ipfsHash);
        } else if (tier == 2) {
            _ownsUncommonPerCycle[msg.sender][currentCycle] = true;
            emit Uncommonforged(msg.sender, tokenId, ipfsHash);
        } else if (tier == 3) {
            _ownsRarePerCycle[msg.sender][currentCycle] = true;
            emit Rareforged(msg.sender, tokenId, ipfsHash);
        } else if (tier == 4) {
            _ownsEpicPerCycle[msg.sender][currentCycle] = true;
            emit Epicforged(msg.sender, tokenId, ipfsHash);
        } else if (tier == 5) {
            _ownsLegendaryPerCycle[msg.sender][currentCycle] = true;
            emit Legendaryforged(msg.sender, tokenId, ipfsHash);
            if (
                _ownsCommonPerCycle[msg.sender][currentCycle] &&
                _ownsUncommonPerCycle[msg.sender][currentCycle] &&
                _ownsRarePerCycle[msg.sender][currentCycle] &&
                _ownsEpicPerCycle[msg.sender][currentCycle] &&
                _ownsLegendaryPerCycle[msg.sender][currentCycle]
            ) {
                emit CycleCompleted(msg.sender, currentCycle);
                _userCycles[msg.sender] += 1;
            }
        } else if (tier == 6) {
            _ownsEternal[msg.sender] = true;
            emit Eternalforged(msg.sender, tokenId, ipfsHash);
        }

        _allocationProcessing();
    }

    function getforgedCounts()
        public
        view
        returns (
            uint256 commonforged,
            uint256 commonRemaining,
            uint256 uncommonforged,
            uint256 uncommonRemaining,
            uint256 rareforged,
            uint256 rareRemaining,
            uint256 epicforged,
            uint256 epicRemaining,
            uint256 legendaryforged,
            uint256 legendaryRemaining,
            uint256 eternalforged,
            uint256 eternalRemaining
        )
    {
        commonforged = _commonCounter.current();
        commonRemaining = commonSupply - commonforged;

        uncommonforged = _uncommonCounter.current();
        uncommonRemaining = uncommonSupply - uncommonforged;

        rareforged = _rareCounter.current();
        rareRemaining = rareSupply - rareforged;

        epicforged = _epicCounter.current();
        epicRemaining = epicSupply - epicforged;

        legendaryforged = _legendaryCounter.current();
        legendaryRemaining = legendarySupply - legendaryforged;

        eternalforged = _eternalCounter.current();
        eternalRemaining = eternalSupply - eternalforged;
    }

    function _setTokenURI(uint256 tokenId, string memory _tokenURI)
        internal
        virtual
    {
        require(
            _exists(tokenId),
            "ERC721URIStorage: URI set for nonexistent token"
        );
        _tokenURIs[tokenId] = _tokenURI;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
            _exists(tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );
        return _tokenURIs[tokenId];
    }

    function setContractURI(string memory newContractURI) public onlyOwner {
        _contractURI = newContractURI;
    }

    function contractURI() public view returns (string memory) {
        return _contractURI;
    }

    function getTokenIPFSHash(uint256 tokenId)
        public
        view
        returns (string memory)
    {
        require(_exists(tokenId), "NO_SUCH_DOT");
        return _tokenIPFSHashes[tokenId];
    }

    function setCommonPrice(uint256 price) public onlyPadrones {
        commonPrice = price;
    }

    function setUncommonPrice(uint256 price) public onlyPadrones {
        uncommonPrice = price;
    }

    function setRarePrice(uint256 price) public onlyPadrones {
        rarePrice = price;
    }

    function setEpicPrice(uint256 price) public onlyPadrones {
        epicPrice = price;
    }

    function setLegendaryPrice(uint256 price) public onlyPadrones {
        legendaryPrice = price;
    }

    function setEternalPrice(uint256 price) public onlyPadrones {
        eternalPrice = price;
    }


    function updatePadrone(address newPadrone, bool add) public onlyPadrones {
        require(newPadrone != address(0), "Invalid address");

        if (add) {
            require(!isPadrone[newPadrone], "Address already exisits");
            padroneTascas.push(newPadrone);
            isPadrone[newPadrone] = true;
        } else {
            require(isPadrone[newPadrone], "Address invalid");
            isPadrone[newPadrone] = false;

            for (uint256 i = 0; i < padroneTascas.length; i++) {
                if (padroneTascas[i] == newPadrone) {
                    padroneTascas[i] = padroneTascas[padroneTascas.length - 1];
                    padroneTascas.pop();
                    break;
                }
            }
        }

    }

    function updateDistributionTascas(address payable _operatingTasca)
        public
        onlyPadrones
    {
        operatingTasca = _operatingTasca;
    }
}
